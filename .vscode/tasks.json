{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "build",
          "command": "dotnet",
          "type": "process",
          "args": [
              "build",
              "${workspaceFolder}/SignalProvider.sln",
              "/property:GenerateFullPaths=true",
              "/consoleloggerparameters:NoSummary"
          ],
          "problemMatcher": "$msCompile"
      },
      {
          "label": "test",
          "command": "dotnet",
          "type": "process",
          "args": [
              "test",
              "${workspaceFolder}/SignalProvider.sln",
              "/property:GenerateFullPaths=true",
              "/consoleloggerparameters:NoSummary"
          ],
          "problemMatcher": "$msCompile"
      }
  ]
}